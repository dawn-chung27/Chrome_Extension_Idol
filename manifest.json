{ 
  "manifest_version" : 2, 
  "name" : "OMG! It's Oh My Girl!",
  "version" : "1.0", 
  "web_accessible_resources" : [ 
  "images/*.jpg", "images/*.png" 
   ],
   "content_scripts" : [
    {
    "matches" : [
    "<all_urls>"
    ],
    "js" : ["content.js"]
    }
    ],
   "browser_action" : {
    "default_popup" : "popup.html"
    }
  }